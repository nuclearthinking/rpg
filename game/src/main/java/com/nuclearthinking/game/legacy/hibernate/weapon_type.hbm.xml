<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nuclearthinking.game.DAO.WeaponType" table="weapon_types">
        <id name="weaponTypeId" type="java.lang.Integer" column="weapon_type_id">
            <generator class="increment"/>
        </id>

        <property name="weaponTypeName" type="java.lang.String" column="weapon_type_name"/>

    </class>
</hibernate-mapping>