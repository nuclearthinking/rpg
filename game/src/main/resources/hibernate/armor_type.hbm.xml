<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nuclearthinking.game.entity.ArmorType" table="armor_types">
        <id name="armorTypeId" type="java.lang.Integer" column="armor_type_id">
            <generator class="increment"/>
        </id>

        <property name="armorTypeName" type="java.lang.String" column="armor_type_name"/>

    </class>
</hibernate-mapping>